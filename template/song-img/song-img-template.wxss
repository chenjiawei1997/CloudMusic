.song-img {
  width: 200rpx;
  height: 200rpx;
  position: relative;
}
.song-img image {
  width: 100%;
  height: 100%;
  background: #666;
  border-radius: 5px;
}
.song-playcount {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  right: 10rpx;
  color: #fff;
}